#dbox db
CREATE TABLE dbox (id INT NOT NULL AUTO_INCREMENT,email VARCHAR(255) NOT NULL,user VARCHAR(255) NOT NULL,domain VARCHAR(255) NOT NULL,password VARCHAR(255) NOT NULL,PRIMARY KEY (id));
ALTER TABLE dbox  AUTO_INCREMENT = 1;
LOAD DATA LOCAL INFILE  'IMPORTFILEHERE' INTO TABLE dbox FIELDS TERMINATED BY ':' (email,user,domain,password);
#hashpass db
CREATE TABLE hashpass (
id INT NOT NULL AUTO_INCREMENT,
password VARCHAR(255) NOT NULL,
md5sum VARCHAR(255) NOT NULL,
sha1sum VARCHAR(255) NOT NULL,
sha224sum VARCHAR(255) NOT NULL,
sha256sum VARCHAR(255) NOT NULL,
sha384sum VARCHAR(255) NOT NULL,
sha512sum VARCHAR(255) NOT NULL,
lmhash VARCHAR(255) NOT NULL,
ntlmhash VARCHAR(255) NOT NULL,
PRIMARY KEY (id)
);
ALTER TABLE hashpass  AUTO_INCREMENT = 1;
LOAD DATA LOCAL INFILE 'IMPORTFILEHERE' INTO TABLE hashpass 
FIELDS TERMINATED BY ':' 
(
password,
md5sum,
sha1sum,
sha224sum,
sha256sum,
sha384sum,
sha512sum,
lmhash,
ntlmhash
);
